@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hybrid3: <https://materials.hybrid3.duke.edu/materials/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@base <http://example.com/> .




<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../db_to_csv/materials_dataset.csv";
    rml:referenceFormulation ql:CSV
    ];
    

    rr:subjectMap [ rr:template "https://materials.hybrid3.duke.edu/materials/{id}"; 
    rr:class sosa:FeatureOfInterest ];

    rr:predicateObjectMap [
        rr:predicate  rdfs:subClassOf;
        rr:objectMap [ rr:template hybrid3:Hybrid_Database ]
    ] ;

    # rr:predicateObjectMap [
    #     rr:predicate  sosa:hasSample;
    #     rr:objectMap [ rr:template "http://example.com/ns/system/{system_id}" ];

    # ] ;

    rr:predicateObjectMap [
        rr:predicate  hybrid3:has_created;
        rr:objectMap [ rml:reference "created" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  hybrid3:has_updated;
        rr:objectMap [ rml:reference "updated" ]
    ] ;
    
    rr:predicateObjectMap [
        rr:predicate  hybrid3:has_extraction_method;
        rr:objectMap[rml:reference "extraction_method"];
    ] ;


    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_observed_primary_property;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap2>;
        rr:joinCondition [
            rr:child "primary_property_id";
            rr:parent "id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_observed_secondary_property;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap2>;
        rr:joinCondition [
            rr:child "secondary_property_id";
            rr:parent "id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_experimental;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap3>;
        rr:joinCondition [
            rr:child "id";
            rr:parent "dataset_id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_computational;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap4>;
        rr:joinCondition [
            rr:child "id";
            rr:parent "dataset_id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_reference;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap5>;
        rr:joinCondition [
            rr:child "reference_id";
            rr:parent "id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_reference;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap6>;
        rr:joinCondition [
            rr:child "reference_id";
            rr:parent "reference_id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_synthesis;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap7>;
        rr:joinCondition [
            rr:child "id";
            rr:parent "dataset_id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  sosa:hasSample;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap8>;
        rr:joinCondition [
            rr:child "system_id";
            rr:parent "id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  sosa:hasObservation;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap9>;
        rr:joinCondition [
            rr:child "id";
            rr:parent "dataset_id";
        ];
    ] ;
    ];

    rr:predicateObjectMap [
    rr:predicate  sosa:hasObservation;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap10>;
        rr:joinCondition [
            rr:child "id";
            rr:parent "dataset_id";
        ];
    ] ;
    ].




<TripleMap2>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../db_to_csv/materials_property.csv";
    rml:referenceFormulation ql:CSV
    ];

    rr:subjectMap [
        rr:template "https://materials.hybrid3.duke.edu/materials/property/{id}";
        rr:class ssn:Property
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_created ;
    rr:objectMap[rml:reference "created"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_updated ;
    rr:objectMap[rml:reference "updated"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_name ;
    rr:objectMap[rml:reference "name"];
    ] .

