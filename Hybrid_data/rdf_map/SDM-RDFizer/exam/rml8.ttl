@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hybrid3: <https://materials.hybrid3.duke.edu/materials/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@base <http://example.com/> .


<TripleMap1>
a rr:TriplesMap;

rml:logicalSource [ rml:source "../../../db_to_csv/materials_author.csv";
rml:referenceFormulation ql:CSV
];

rr:subjectMap [
    rr:template "http://example.com/ns/author/{id}";
    rr:class foaf:Person
];

rr:predicateObjectMap [
rr:predicate  hybrid3:has_first_name ;
rr:objectMap[rml:reference "first_name"];
];

rr:predicateObjectMap [
rr:predicate  hybrid3:has_last_name ;
rr:objectMap[rml:reference "last_name"];
];

rr:predicateObjectMap [
rr:predicate  hybrid3:has_institution ;
rr:objectMap[rml:reference "institution"];
].




