@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hybrid3: <https://materials.hybrid3.duke.edu/materials/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ex: <http://example.com/ns/>.
@base <http://example.com/> .


<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../db_to_csv/materials_computationaldetails.csv";
    rml:referenceFormulation ql:CSV
    ];

    rr:subjectMap [
        rr:template "http://example.com/ns/computation/{dataset_id}";
    
        rr:class ex:Procedure
    ];

    rr:predicateObjectMap [
        rr:predicate  sosa:has_level_of_theory;
        rr:objectMap [ rml:reference "level_of_theory" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  sosa:has_k_point_grid;
        rr:objectMap [ rml:reference "k_point_grid" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  sosa:has_level_of_relativity;
        rr:objectMap [ rml:reference "level_of_relativity" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  sosa:has_basis_set_definition;
        rr:objectMap [ rml:reference "basis_set_definition" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  sosa:has_numerical_accuracy;
        rr:objectMap [ rml:reference "numerical_accuracy" ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate  sosa:has_xc_functional;
        rr:objectMap [ rml:reference "xc_functional" ]
    ] .

