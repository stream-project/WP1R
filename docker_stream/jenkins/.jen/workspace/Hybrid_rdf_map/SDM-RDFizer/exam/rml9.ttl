<TripleMap1>
a rr:TriplesMap;

rml:logicalSource [ rml:source "../../../Hybrid_db_to_csv/materials_synthesismethod.csv";
rml:referenceFormulation ql:CSV
];

rr:subjectMap [
    rr:template "https://materials.hybrid3.duke.edu/materials/synthesis/{dataset_id}";
    rr:class hybrid3:Synthesis
];

rr:predicateObjectMap [
rr:predicate  hybrid3:has_starting_materials ;
rr:objectMap[rml:reference "starting_materials"];
];

rr:predicateObjectMap [
rr:predicate  hybrid3:has_product ;
rr:objectMap[rml:reference "product"];
].