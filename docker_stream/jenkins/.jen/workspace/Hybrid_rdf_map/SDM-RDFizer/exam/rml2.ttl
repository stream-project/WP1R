@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hybrid3: <https://materials.hybrid3.duke.edu/materials/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@base <http://example.com/> .



<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../Hybrid_db_to_csv/materials_property.csv";
    rml:referenceFormulation ql:CSV
    ];

    rr:subjectMap [
        rr:template "https://materials.hybrid3.duke.edu/materials/property_{id}";
         rr:class ssn:Property
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_created_by ;
    rr:objectMap[rr:template "p_{created_by_id}";
                 rr:termType rr:Literal];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_updated_by ;
    rr:objectMap[rr:template "p_{updated_by_id}";
                 rr:termType rr:Literal];
    ] .

    