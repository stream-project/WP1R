@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hybrid3: <https://materials.hybrid3.duke.edu/materials/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@base <http://example.com/> .

  <TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../Hybrid_db_to_csv/materials_reference.csv";
    rml:referenceFormulation ql:CSV
    ];

    rr:subjectMap [
        rr:template "https://materials.hybrid3.duke.edu/materials/reference/{id}";
        rr:class hybrid3:Reference
    ];

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_title ;
    rr:objectMap[rml:reference "title"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_journal ;
    rr:objectMap[
        rml:reference "journal"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_vol ;
    rr:objectMap[rml:reference "vol"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_pages_start ;
    rr:objectMap[rml:reference "pages_start"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_pages_end ;
    rr:objectMap[rml:reference "pages_end"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_year ;
    rr:objectMap[rml:reference "year"];
    ] ;

    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_doi_isbn ;
    rr:objectMap[rml:reference "doi_isbn"];
    ] .

