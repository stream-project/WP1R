@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns>.
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix hybrid3: <https://materials.hybrid3.duke.edu/materials/> .
@prefix matonto: <http://matonto.org/ontology/matonto#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@base <http://example.com/> .


<TripleMap1>
    a rr:TriplesMap;

    rml:logicalSource [ rml:source "../../../Hybrid_db_to_csv/materials_author_references.csv";
    rml:referenceFormulation ql:CSV
    ];

    rr:subjectMap [
        rr:template "https://materials.hybrid3.duke.edu/materials/reference/{reference_id}";
        rr:class hybrid3:Reference
    ];



    rr:predicateObjectMap [
    rr:predicate  hybrid3:has_author;
    rr:objectMap[
        rr:parentTriplesMap <TripleMap2>;
        rr:joinCondition [
            rr:child "author_id";
            rr:parent "id";
        ];
    ] ;
    ].

<TripleMap2>
a rr:TriplesMap;

rml:logicalSource [ rml:source "../../../Hybrid_db_to_csv/materials_author.csv";
rml:referenceFormulation ql:CSV
];

rr:subjectMap [
    rr:template "https://materials.hybrid3.duke.edu/materials/author/{id}";
    rr:class foaf:Person
].